{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "buildCommand": "next build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "functions": {
    "app/**/*.tsx": {
      "maxDuration": 60
    },
    "app/**/*.ts": {
      "maxDuration": 60
    }
  }
}
